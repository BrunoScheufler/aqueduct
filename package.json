{
  "name": "aqueduct",
  "version": "1.2.0",
  "description": "A cloud-native GraphQL-powered gateway service",
  "main": "build/index.js",
  "repository": "https://github.com/BrunoScheufler/aqueduct",
  "author": "Bruno Scheufler",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@types/graphql": "14.0.5",
    "@types/node-fetch": "2.1.4",
    "apollo-link": "1.2.6",
    "apollo-link-context": "1.0.12",
    "apollo-link-http": "1.5.9",
    "apollo-server-express": "2.3.2",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-jwt": "5.3.1",
    "fs-extra": "7.0.1",
    "graphql": "14.1.1",
    "graphql-tools": "3.1.1",
    "helmet": "3.15.0",
    "isomorphic-unfetch": "2.1.1",
    "js-yaml": "3.13.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.4",
    "@types/express": "4.16.1",
    "@types/express-jwt": "0.0.40",
    "@types/fs-extra": "5.0.4",
    "@types/helmet": "0.0.42",
    "@types/js-yaml": "3.12.0",
    "@types/node": "10.12.18",
    "ts-node-dev": "1.0.0-pre.32",
    "tslint": "5.12.1",
    "typescript": "3.2.4"
  },
  "scripts": {
    "build": "tsc --project .",
    "test": "yarn run build && yarn run lint",
    "lint": "tslint --project .",
    "start": "node ./build/index.js",
    "dev": "ts-node-dev ./src/index.ts",
    "docker:build": "docker build -t brunoscheufler/aqueduct:1.2.0 .",
    "docker:tag": "docker tag brunoscheufler/aqueduct:1.2.0 brunoscheufler/aqueduct:latest",
    "docker:publish": "docker push brunoscheufler/aqueduct:1.2.0 && docker push brunoscheufler/aqueduct:latest",
    "docker": "yarn docker:build && yarn docker:tag && yarn docker:publish"
