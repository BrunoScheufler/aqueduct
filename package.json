{
  "name": "aqueduct",
  "version": "0.0.0-development",
  "description": "A cloud-native GraphQL-powered gateway service",
  "main": "build/index.js",
  "repository": "https://github.com/BrunoScheufler/aqueduct",
  "author": "Bruno Scheufler",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "apollo-link": "1.2.13",
    "apollo-link-context": "1.0.19",
    "apollo-link-http": "1.5.16",
    "apollo-server-express": "2.9.5",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "fs-extra": "7.0.1",
    "graphql": "14.5.8",
    "graphql-tools": "4.0.5",
    "helmet": "3.21.1",
    "isomorphic-unfetch": "3.0.0",
    "js-yaml": "3.13.1"
  },
  "devDependencies": {
    "@types/graphql": "14.2.3",
    "@types/node-fetch": "2.5.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.1",
    "@types/express-jwt": "0.0.40",
    "@types/fs-extra": "5.1.0",
    "@types/helmet": "0.0.44",
    "@types/js-yaml": "3.12.1",
    "@types/node": "10.14.21",
    "prettier": "1.18.2",
    "semantic-release": "15.13.24",
    "semantic-release-docker": "2.2.0",
    "ts-node-dev": "1.0.0-pre.43",
    "tslint": "5.20.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "build": "tsc --project .",
    "lint:style": "prettier --check src/**/*.ts",
    "lint:code": "tslint --project .",
    "lint": "yarn lint:style && yarn lint:code",
    "watch": "tsc --project . --watch",
    "test": "echo \"Currently, there are no tests, this placeholder was added so the current step won't fail.\"",
    "semantic-release": "semantic-release"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "semantic-release-docker",
        {
          "name": "brunoscheufler/aqueduct"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
